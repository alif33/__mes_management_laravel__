{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "s": "php artisan serve",
        "mr": "php artisan migrate",
        "mrf": "php artisan migrate:fresh",
        "seed": "php artisan db:seed",
        "rb": "php artisan migrate:rollback --step=15",
        "refresh": "yarn rb && yarn mr && yarn seed",
        "make": "php artisan make:model Request -cfms",
        "next": ""
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14"
    }
}
